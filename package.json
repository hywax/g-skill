{
  "name": "skillq",
  "type": "module",
  "version": "0.3.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "bin": {
    "skillq": "./cli/index.mjs"
  },
  "scripts": {
    "build": "nuxt build && pnpm run cli:meta --prod",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run build && changelogen --release --push",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxt typecheck",
    "clean": "rm -rf dist",
    "clean:modules": "rm -rf .nuxt node_modules",
    "cli:security": "node cli/index.mjs security",
    "cli:meta": "node cli/index.mjs meta",
    "cli:optimize": "node cli/index.mjs optimize",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/ui": "^3.0.0-alpha.9",
    "@nuxt/ui-pro": "^3.0.0-alpha.9",
    "@nuxtjs/i18n": "9.1.1",
    "@vueuse/nuxt": "12.0.0",
    "nuxt": "^3.14.1592",
    "nuxt-zod-i18n": "1.11.2",
    "octokit": "^4.0.2",
    "vue3-marquee": "^4.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@hywax/tools-eslint": "^1.1.0",
    "@iconify-json/circle-flags": "^1.2.1",
    "@iconify-json/lucide": "^1.2.18",
    "@iconify-json/simple-icons": "^1.2.14",
    "@types/node": "^22.10.1",
    "changelogen": "^0.5.7",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "typescript": "^5.7.2",
    "vue-tsc": "^2.1.10"
  },
  "lint-staged": {
    "*.ts": "pnpm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
